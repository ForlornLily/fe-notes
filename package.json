{
  "name": "fe-notes",
  "version": "1.0.0",
  "description": "fe-notes",
  "main": "index.js",
  "author": "Forlorn",
  "license": "MIT",
  "scripts": {
    "dev": "vuepress dev docs",
    "build": "vuepress build docs",
    "lint": "prettier --write 'docs/**/*.md'",
    "release": "node scripts/release.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run release && git add . && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "preserve": "never"
  },
  "devDependencies": {
    "gray-matter": "^4.0.2",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "markdown-it-include": "^1.1.0",
    "markdown-toc": "^1.2.0",
    "prettier": "^2.0.1",
    "vuepress": "^1.4.0"
  },
  "dependencies": {}
}
